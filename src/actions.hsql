#ifndef ACTIONS
#define ACTIONS
#include "shared.hsql"
#include "categories.hsql"
CREATE TABLE __TABLE_PREFIX(actions) (
  id INT NOT NULL PRIMARY KEY,
  created_at DATETIME NOT NULL,
  category_id INT NULL,
  type TEXT CHECK(type IN ('INSERT','DELETE','CREATE')),
  FOREIGN KEY (category_id)
    REFERENCES __TABLE_PREFIX(categories)(id)
    ON DELETE SET NULL
);
#endif