#ifndef VALUES_H
#define VALUES_H
#include "shared.hsql"
#include "columns.hsql"
#include "changes.hsql"
CREATE TABLE __TABLE_PREFIX(values) (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  column_id INTEGER NOT NULL,
  change_id INTEGER NOT NULL,
  old_value TEXT NULL,
  new_value TEXT NULL,
  FOREIGN KEY (column_id)
    REFERENCES __TABLE_PREFIX(columns)(id)
    ON DELETE CASCADE,
  FOREIGN KEY (change_id)
    REFERENCES __TABLE_PREFIX(changes)(id)
    ON DELETE CASCADE
);
#endif