#ifndef COLUMNS_H
#define COLUMNS_H
#include "shared.hsql"
#include "tables.hsql"
CREATE TABLE __TABLE_PREFIX(columns) (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL,
  table_id INTEGER NOT NULL,
  UNIQUE (table_id, name),
  FOREIGN KEY(table_id)
    REFERENCES __TABLE_PREFIX(tables)(id)
    ON DELETE CASCADE
);
#endif