{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowUnreachableCode": false,
    "skipLibCheck": true,
    "rootDir": ".",
    "typeRoots": ["@types"],
    "types": ["raw-loader.d.ts", "node"]
  },
  "include": ["**/*.ts"],
}
