class Category {
  name: string
  icon: string
}

class Action {
  createdAt: Date
}

class Change {
  type: ChangeType
  rowId: int
  orderIndex: int
}

enum ChangeType {
  Insert
  Update
  Delete
}

class Table {
  name: string
}

class Column {
  name: string
  type: string
}

class Value {
  oldValue: string
  newValue: string
}

class Channel {
  id: int
}

Action o-- Category
Action o-- Change
Change o-- Value
Change o-- ChangeType
Column o-down- Table 
Value o-- Column
Channel o-- Action
Table o-- Channel