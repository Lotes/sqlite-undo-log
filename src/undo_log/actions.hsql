#ifndef ACTIONS_H
#define ACTIONS_H
#include "shared.hsql"
#include "categories.hsql"
CREATE TABLE __TABLE_PREFIX(actions) (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  created_at DATETIME NOT NULL,
  category_id INTEGER NULL,
  FOREIGN KEY (category_id)
    REFERENCES __TABLE_PREFIX(categories)(id)
    ON DELETE SET NULL
);
#endif